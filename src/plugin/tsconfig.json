{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "moduleResolution": "nodenext",
        "outDir": "../../dist",
        "allowJs": true,
        "checkJs": true,
        "strict": true,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true
    },
    "composite": true ,
    "paths": {
        "jsonata": ["node_modules/jsonata"] 
    },
    "include": [
        "./index.ts",
        "./index.html"
    ]
}